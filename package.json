{
  "name": "express-ts",
  "version": "1.0.0",
  "description": "express learning server",
  "main": "main.js",
  "type": "commonjs",
  "scripts": {
    "start": "node dist/main.js",
    "build": "tsc",
    "dev": "nodemon",
    "dev:inspect": "nodemon -e ts,json --exec node --inspect=localhost:9229 -r ts-node/register src/main.ts",
    "test": "jest",
    "test:e2e": "jest --config jest.e2e.config.ts ",
    "lint": "eslint ./src/**",
    "lint:fix": "eslint ./src/** --fix",
    "generate": "npx prisma generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/philipp-moriss/express-ts.git"
  },
  "author": "philipp moriss",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/philipp-moriss/express-ts/issues"
  },
  "homepage": "https://github.com/philipp-moriss/express-ts#readme",
  "dependencies": {
    "@prisma/client": "^3.15.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "inversify": "^6.0.1",
    "jsonwebtoken": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "tslog": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.4",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.2",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "prisma": "^3.15.2",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  }
}
